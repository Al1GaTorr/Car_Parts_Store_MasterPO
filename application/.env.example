# Example .env for startap-master
# Copy this file to .env and fill values as needed.

# Backend server port
PORT=8080
MONGO_URI=mongodb://localhost:27017/bazarPO

# 2GIS API key (optional for dev)
TWOGIS_API_KEY=60bd214c-bcab-413a-a891-ffba2e40a31f

